<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="rootBox" alignment="CENTER" prefHeight="600.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.gian.App.Infrastructure.UI.Components.Register.RegisterController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Label text="Ingrese su nombre de usuario:">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Label>
      <TextField fx:id="nameField" alignment="CENTER" maxWidth="100.0" minHeight="30.0" minWidth="100.0" prefWidth="100.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </TextField>
      <Button fx:id="nextButton" minHeight="30.0" mnemonicParsing="false" text="Ingresar" />
   </children>
</VBox>
